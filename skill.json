{
  "manifest": {
    "publishingInformation": {
      "locales": {
        "en-GB": {
          "summary": "Learn fun facts about your local area.",
          "examplePhrases": [
            "Alexa open local facts",
            "Alexa ask local facts for a fact",
            "Alexa tell local facts to tell me a fact"
          ],
          "keywords": [
            "local",
            "facts",
            "information"
          ],
          "name": "Local Facts",
          "description": "Ask for facts about your local area based on your UK county!"
        }
      },
      "category": "KNOWLEDGE_AND_TRIVIA"
    },
    "apis": {
      "custom": {
        "endpoint": {
          "sourceDir": "lambda/custom",
          "uri": "ask-custom-Local-Facts-default"
        }
      }
    },
    "manifestVersion": "1.0",
    "permissions": [
      {
        "name": "alexa::devices:all:address:full:read"
      }
    ]
  }
}
